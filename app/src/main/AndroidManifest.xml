        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ui.TabActivity" />
        <activity android:name=".view.ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ui.FormsActivity" />
        <activity android:name=".view.ui.Form1Activity"></activity>
        <activity android:name=".view.ui.Form2Activity"></activity>
        <activity android:name=".view.ui.Form3Activity"></activity>
        <activity android:name=".view.ui.Form4Activity"></activity>
        <activity android:name=".view.ui.Form5aActivity"></activity>
    </application>

</manifest>